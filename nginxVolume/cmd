sudo docker run --name nginx -p 192.168.1.12:443:443 -v /home/g8/bnss-g8/BNSS-Docker-Volume/nginxVolume/conf/nginx.conf:/etc/nginx/nginx.conf -v /home/g8/bnss-g8/BNSS-Docker-Volume/nginxVolume/conf/conf.d:/etc/nginx/conf.d -v /home/g8/bnss-g8/BNSS-Docker-Volume/nginxVolume/html:/usr/share/nginx/html -d nginx 

