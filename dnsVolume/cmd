docker run --name dns -d -p 53:53/udp -p 53:53/tcp -p 853:853/tcp -v /home/g8/bnss-g8/BNSS-Docker-Volume/dnsVolume/unbound/unbound.conf:/opt/unbound/etc/unbound/unbound.conf:ro -v /home/g8/bnss-g8/BNSS-Docker-Volume/dnsVolume/unbound/tls:/opt/unbound/etc/unbound/tls -v /home/g8/bnss-g8/BNSS-Docker-Volume/dnsVolume/unbound/logs:/opt/unbound/unbound.log --restart=always redbirdyi/nds



docker run --name dns11 -d -p 192.168.1.11:53:53/udp -p 192.168.1.11:53:53/tcp -p 192.168.1.11:853:853/tcp -v /home/g8/bnss-g8/BNSS-Docker-Volume/dnsVolume/unbound/unbound.conf:/opt/unbound/etc/unbound/unbound.conf:ro -v /home/g8/bnss-g8/BNSS-Docker-Volume/dnsVolume/unbound/tls:/opt/unbound/etc/unbound/tls -v /home/g8/bnss-g8/BNSS-Docker-Volume/dnsVolume/unbound/logs:/opt/unbound/unbound.log --restart=always redbirdyi/nds

